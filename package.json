{
  "name": "YomaMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "_git:pre-commit": "pretty-quick --staged",
    "_git:pre-push": "npm run code:lint",
    "init:ios": "npx pod-install",
    "init": "npm-run-all env:dev init:ios",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest --ci --config jest.node.config.js --verbose -w 1",
    "test:watch": "npm run test -- --watch",
    "test:ui": "jest --ci --config jest.ui.config.js --verbose",
    "test:ui:watch": "npm run test:ui -- --watch",
    "lint": "eslint --ignore-path .gitignore --ignore-path .eslintignore .",
    "format": "prettier --write .",
    "npm:audit": "audit-ci --critical",
    "_code:lint:prettier": "prettier --check .",
    "_code:lint:eslint": "eslint .",
    "code:lint": "npm-run-all _code:lint:prettier _code:lint:eslint",
    "help": "npx package-json-help.js"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "ramda": "^0.27.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.0.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@trivago/prettier-plugin-sort-imports": "^1.4.4",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "husky": "^4.3.5",
    "jest": "^25.1.0",
    "jest-watch-typeahead": "^0.6.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "npm-run-all": "^4.1.5",
    "package-json-help": "0.0.10",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^16"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
