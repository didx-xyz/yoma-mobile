{
  "name": "YomaMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "_code:lint:eslint": "eslint .",
    "_code:lint:prettier": "prettier --check .",
    "_git:pre-commit": "pretty-quick --staged",
    "_git:pre-push": "npm-run-all code:lint",
    "android": "react-native run-android",
    "bundle:android": "react-native bundle --platform android --entry-file index.js --bundle-output android.bundle --verbose",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "code:lint": "npm-run-all _code:lint:prettier _code:lint:eslint",
    "code:fix": "npm-run-all format lint:fix",
    "create:keystore": "echo $YOMA_UPLOAD_KEYSTORE_BASE64 | openssl base64 -d -A > android/app/$YOMA_UPLOAD_STORE_FILE",
    "deploy:alpha:android": "cd android && bundle exec fastlane beta_deploy",
    "deploy:release:android": "cd android && bundle exec fastlane production_deploy",
    "make_env:dev": "node env/generate.js dev",
    "make_env:test": "node env/generate.js test",
    "format": "prettier --write .",
    "help": "npx package-json-help.js",
    "init": "npm i && npm-run-all env:dev init:ios",
    "init:ios": "npx pod-install",
    "ios": "react-native run-ios",
    "lint": "npx eslint .  --ignore-path .eslintignore",
    "lint:check": "npm run lint",
    "lint:fix": "npm run lint --fix",
    "lint:markdown": "npx markdownlint */*.md",
    "npm:audit": "audit-ci --critical",
    "reset": "npx rimraf node_modules && npm i && npx pod-install",
    "reset:all": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules  && npm cache clean --force && npm install && npm start -- --reset-cache",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "start:reset:all": "npm-run-all reset:all start:reset",
    "test": "jest --ci --config jest.node.config.js --verbose -w 1",
    "test:ui": "jest --ci --config jest.ui.config.js --verbose",
    "test:ui:watch": "npm run test:ui -- --watch",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.17",
    "@react-native-community/datetimepicker": "^3.4.3",
    "@react-native-firebase/app": "^12.0.0",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.27.1",
    "common-tags": "^1.8.2",
    "date-fns": "^2.28.0",
    "expo-secure-store": "^10.2.0",
    "formik": "^2.2.6",
    "i18next": "^21.6.16",
    "intl-pluralrules": "^1.3.1",
    "ramda": "^0.27.2",
    "react": "17.0.2",
    "react-i18next": "^11.16.7",
    "react-native": "0.66.0",
    "react-native-app-auth": "^6.4.3",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-document-picker": "^8.1.0",
    "react-native-dropdown-picker": "^5.4.0",
    "react-native-elements": "^3.3.2",
    "react-native-flash-message": "^0.2.1",
    "react-native-flipper": "^0.143.0",
    "react-native-gesture-handler": "^2.4.1",
    "react-native-image-crop-picker": "^0.37.3",
    "react-native-localize": "^2.2.1",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-unimodules": "^0.14.10",
    "react-native-vector-icons": "^9.1.0",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/runtime": "^7.17.9",
    "@react-native-community/eslint-config": "^3.0.1",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/common-tags": "^1.8.1",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/form-data": "2.5.0",
    "@types/jest": "^27.4.1",
    "@types/react-native": "^0.66.0",
    "@types/react-test-renderer": "^17.0.2",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "audit-ci": "^6.2.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-module-resolver": "^1.4.0",
    "firebase-tools": "^9.23.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^1.1.0",
    "metro-react-native-babel-preset": "^0.70.2",
    "npm-run-all": "^4.1.5",
    "package-json-help": "0.0.11",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "17.0.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-flipper": "^2.0.1",
    "typescript": "^4.6.3"
  },
  "react-native-unimodules": {
    "android": {
      "modulesPaths": [
        "../../node_modules"
      ]
    }
  },
  "resolutions": {
    "@types/react": "^16"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native)/.*)(?!(@react-navigation|react-navigation)/.*)(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "setupFiles": [
      "<rootDir>/tests/jest.setup.js",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run _git:pre-commit",
      "pre-push": "npm run _git:pre-push"
    }
  }
}
